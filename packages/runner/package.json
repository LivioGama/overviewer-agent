{
  "name": "@overviewer-agent/runner",
  "version": "0.1.0",
  "private": true,
  "description": "Task execution engine for running automation jobs",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "bun test",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t overviewer-runner .",
    "docker:run": "docker run --rm overviewer-runner"
  },
  "dependencies": {
    "@octokit/auth-app": "^6.0.0",
    "@octokit/rest": "^20.0.0",
    "@overviewer-agent/shared": "workspace:*",
    "axios": "^1.11.0",
    "fs-extra": "^11.0.0",
    "redis": "^4.6.0",
    "simple-git": "^3.20.0",
    "uuid": "^9.0.0",
    "yaml": "^2.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/uuid": "^9.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}
